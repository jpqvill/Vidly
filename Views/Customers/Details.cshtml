@model Vidly.Models.Customer
@{
    ViewBag.Title = "Details";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>@Model.Name</h2>
<li>
    Membership Type: @Model.MembershipType.DiscountName
</li>
@if (Model.BirthDate != null)
{
    <li>
        Birthdate: @Convert.ToDateTime(Model.BirthDate).ToString("MM/dd/yyyy")
    </li>
}


